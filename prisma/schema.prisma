generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}



model User {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  email   String
  name    String?
  address Address?
}
type Address {
  street String
  city   String
  state  String
  zip    String
}